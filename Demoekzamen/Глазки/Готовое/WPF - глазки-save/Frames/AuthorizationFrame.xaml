<Page x:Class="WPF___глазки_save.Frames.AuthorizationFrame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF___глазки_save.Frames"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AuthorizationFrame">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="150">
            <TextBlock>Логин</TextBlock>
            <TextBox x:Name="TextBoxLogin"></TextBox>
            <TextBlock>Пароль</TextBlock>
            <PasswordBox Name="TextBoxPass"></PasswordBox>
            <Button x:Name="SignIn" Click="SignIn_Click">Вход</Button>
        </StackPanel>
    </Grid>
</Page>
